  function @X@_vect_nrm2_weight(n,x,w,aux) result(res)
    use psi_serial_mod
    implicit none
    class(psb_@X@_vect_type), intent(inout) :: x
    class(psb_@X@_vect_type), intent(inout) :: w
    class(psb_@X@_vect_type), intent(inout), optional :: aux
    integer(psb_ipk_), intent(in)           :: n
    @RTYPE@(@FKIND@)                        :: res
    integer(psb_ipk_)                       :: info

    ! Temp vectors
    type(psb_@X@_vect_type) :: wtemp

    if( allocated(w%v) ) then
      if (.not.present(aux)) then
        allocate(wtemp%v, mold=w%v)
        call wtemp%v%bld(w%get_vect())
      else
        call psb_geaxpby(n,@XONE@,w%v%v,@XZERO@,aux%v%v,info)
      end if
    else
      info = -1
    end if
    if (info /= 0 ) then
      res = -@RONE@
      return
    end if

    if (allocated(x%v)) then
      if (.not.present(aux)) then
        call wtemp%v%mlt(x%v,info)
        res = wtemp%v%nrm2(n)
      else
        call aux%v%mlt(x%v,info)
        res = aux%v%nrm2(n)
      end if
    else
      res = @RZERO@
    end if

    if (.not.present(aux)) then
      call wtemp%free(info)
    end if

  end function @X@_vect_nrm2_weight

  function @X@_vect_nrm2_weight_mask(n,x,w,id,info,aux) result(res)
    use psi_serial_mod
    implicit none
    class(psb_@X@_vect_type), intent(inout) :: x
    class(psb_@X@_vect_type), intent(inout) :: w
    class(psb_@X@_vect_type), intent(inout) :: id
    integer(psb_ipk_), intent(in)           :: n
    @RTYPE@(@FKIND@)                        :: res
    integer(psb_ipk_), intent(out)          :: info
    class(psb_@X@_vect_type), intent(inout), optional :: aux

    ! Temp vectors
    type(psb_@X@_vect_type) :: wtemp

    if( allocated(w%v) ) then
      if (.not.present(aux)) then
        allocate(wtemp%v, mold=w%v)
        call wtemp%v%bld(w%get_vect())
      else
        call psb_geaxpby(n,@XONE@,w%v%v,@XZERO@,aux%v%v,info)
      end if
    else
      info = -1
    end if
    if (info /= 0 ) then
      res = -@RONE@
      return
    end if


    if (allocated(x%v).and.allocated(id%v)) then
      if (.not.present(aux)) then
        where( abs(id%v%v) <= @RZERO@) wtemp%v%v = @RZERO@
        call wtemp%set_host()
        call wtemp%v%mlt(x%v,info)
        res = wtemp%v%nrm2(n)
      else
        where( abs(id%v%v) <= @RZERO@) aux%v%v = @RZERO@
        call aux%set_host()
        call aux%v%mlt(x%v,info)
        res = aux%v%nrm2(n)
      end if
    else
      res = @RZERO@
    end if

    if (.not.present(aux)) then
      call wtemp%free(info)
    end if

  end function @X@_vect_nrm2_weight_mask

