  subroutine @X@_vect_mask_a(c,x,m,t,info)
    use psi_serial_mod
    implicit none
    @RTYPE@(@FKIND@), intent(inout)          :: c(:)
    @RTYPE@(@FKIND@), intent(inout)          :: x(:)
    logical, intent(out)                     :: t;
    class(psb_@X@_vect_type), intent(inout)  :: m
    integer(psb_ipk_), intent(out)           :: info

    info = 0
