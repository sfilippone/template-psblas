! File: mld_@X@_prec_mod.f90
!
! Module: mld_@X@_prec_mod
!
!  This module defines the interfaces to the real/complex, single/double
!  precision versions of the user-level MLD2P4 routines.
!
module mld_@X@_prec_mod

  use mld_@X@_prec_type
  use mld_@X@_jac_smoother
  use mld_@X@_as_smoother
  use mld_@X@_id_solver
  use mld_@X@_diag_solver
  use mld_@X@_ilu_solver
    
  interface mld_precinit
    subroutine mld_@X@precinit(p,ptype,info,nlev)
      import :: psb_@X@spmat_type, psb_desc_type, @FKIND@, &
           & mld_@X@prec_type, psb_ipk_
      type(mld_@X@prec_type), intent(inout)    :: p
      character(len=*), intent(in)             :: ptype
      integer(psb_ipk_), intent(out)           :: info
      integer(psb_ipk_), optional, intent(in)  :: nlev
    end subroutine mld_@X@precinit
  end interface

  interface mld_precset
    module procedure mld_@X@_iprecsetsm, mld_@X@_iprecsetsv, &
         & mld_@X@_iprecseti, mld_@X@_iprecsetc, mld_@X@_iprecsetr, &
         & mld_@X@_cprecseti, mld_@X@_cprecsetc, mld_@X@_cprecsetr
  end interface

!!$  interface mld_inner_precset

  interface mld_precbld
    subroutine mld_@X@precbld(a,desc_a,prec,info,amold,vmold,imold)
      import :: psb_@X@spmat_type, psb_desc_type, @FKIND@, &
           & psb_@X@_base_sparse_mat, psb_@X@_base_vect_type, &
           & psb_i_base_vect_type, mld_@X@prec_type, psb_ipk_
      implicit none
      type(psb_@X@spmat_type), intent(in), target          :: a
      type(psb_desc_type), intent(inout), target           :: desc_a
      type(mld_@X@prec_type), intent(inout), target        :: prec
      integer(psb_ipk_), intent(out)                       :: info
      class(psb_@X@_base_sparse_mat), intent(in), optional :: amold
      class(psb_@X@_base_vect_type), intent(in), optional  :: vmold
      class(psb_i_base_vect_type), intent(in), optional  :: imold
!!$      character, intent(in),optional             :: upd
    end subroutine mld_@X@precbld
  end interface

contains

  subroutine mld_@X@_iprecsetsm(p,val,info)
    type(mld_@X@prec_type), intent(inout)    :: p
    class(mld_@X@_base_smoother_type), intent(in)   :: val
    integer(psb_ipk_), intent(out)           :: info

    call p%set(val,info)
  end subroutine mld_@X@_iprecsetsm

  subroutine mld_@X@_iprecsetsv(p,val,info)
    type(mld_@X@prec_type), intent(inout)    :: p
    class(mld_@X@_base_solver_type), intent(in)   :: val
    integer(psb_ipk_), intent(out)                :: info

    call p%set(val,info)
  end subroutine mld_@X@_iprecsetsv

  subroutine mld_@X@_iprecseti(p,what,val,info)
    type(mld_@X@prec_type), intent(inout)    :: p
    integer(psb_ipk_), intent(in)            :: what 
    integer(psb_ipk_), intent(in)            :: val
    integer(psb_ipk_), intent(out)           :: info

    call p%set(what,val,info)
  end subroutine mld_@X@_iprecseti

  subroutine mld_@X@_iprecsetr(p,what,val,info)
    type(mld_@X@prec_type), intent(inout)    :: p
    integer(psb_ipk_), intent(in)            :: what 
    real(@FKIND@), intent(in)             :: val
    integer(psb_ipk_), intent(out)           :: info

    call p%set(what,val,info)
  end subroutine mld_@X@_iprecsetr

  subroutine mld_@X@_iprecsetc(p,what,val,info)
    type(mld_@X@prec_type), intent(inout)   :: p
    integer(psb_ipk_), intent(in)           :: what 
    character(len=*), intent(in)            :: val
    integer(psb_ipk_), intent(out)          :: info

    call p%set(what,val,info)
  end subroutine mld_@X@_iprecsetc

  subroutine mld_@X@_cprecseti(p,what,val,info)
    type(mld_@X@prec_type), intent(inout)   :: p
    character(len=*), intent(in)            :: what 
    integer(psb_ipk_), intent(in)           :: val
    integer(psb_ipk_), intent(out)          :: info

    call p%set(what,val,info)
  end subroutine mld_@X@_cprecseti

  subroutine mld_@X@_cprecsetr(p,what,val,info)
    type(mld_@X@prec_type), intent(inout)   :: p
    character(len=*), intent(in)            :: what 
    real(@FKIND@), intent(in)             :: val
    integer(psb_ipk_), intent(out)          :: info

    call p%set(what,val,info)
  end subroutine mld_@X@_cprecsetr

  subroutine mld_@X@_cprecsetc(p,what,val,info)
    type(mld_@X@prec_type), intent(inout)   :: p
    character(len=*), intent(in)            :: what 
    character(len=*), intent(in)            :: val
    integer(psb_ipk_), intent(out)          :: info

    call p%set(what,val,info)
  end subroutine mld_@X@_cprecsetc

end module mld_@X@_prec_mod
