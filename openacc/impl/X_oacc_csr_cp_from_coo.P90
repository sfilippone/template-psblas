submodule (psb_@X@_oacc_csr_mat_mod) psb_@X@_oacc_csr_cp_from_coo_impl
  use psb_base_mod
contains
  module subroutine psb_@X@_oacc_csr_cp_from_coo(a, b, info)
    implicit none

    class(psb_@X@_oacc_csr_sparse_mat), intent(inout) :: a
    class(psb_@X@_coo_sparse_mat), intent(in)         :: b
    integer(psb_ipk_), intent(out)                  :: info

    info = psb_success_

    call a%psb_@X@_csr_sparse_mat%cp_from_coo(b, info)
    if (info /= 0) goto 9999

    call a%set_dev()
    if (info /= 0) goto 9999

    return

9999 continue
    info = psb_err_alloc_dealloc_
    return

  end subroutine psb_@X@_oacc_csr_cp_from_coo
end submodule psb_@X@_oacc_csr_cp_from_coo_impl
